{
  "name": "tobiasbu",
  "version": "3.0.0",
  "description": "my website project",
  "type": "module",
  "engines": {
    "node": ">=20.x.x"
  },
  "scripts": {
    "dev:clean": "rm -rf dist",
    "dev:run": "ELEVENTY_ENV=development && eleventy --serve",
    "start": "npm run dev:clean && npm run dev:run",
    "jekyll": "bundle exec jekyll serve --livereload --config _config.yml,_config_dev.yml",
    "jekyll-trace": "bundle exec jekyll serve --livereload --trace --config _config.yml,_config_dev.yml",
    "css": "postcss css/*.css --dir _includes/css/ --watch true",
    "css-build": "postcss css/*.css --dir _includes/css/",
    "build": "npm run css-build && bundle exec jekyll build",
    "deploy": "node deploy.js",
    "update": "npm run clean && npm run build && npm run deploy",
    "clean": "rm -rf node_modules/gh-pages/.cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobiasbu/portfolio.git"
  },
  "keywords": [
    "website",
    "portfolio",
    "tobiasbu",
    "tbu"
  ],
  "author": "tobiasbu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tobiasbu/portfolio/issues"
  },
  "homepage": "https://github.com/tobiasbu",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0-beta.1",
    "autoprefixer": "^10.4.20",
    "cssnano": "^5.1.15",
    "gh-pages": "^6.1.1",
    "git-last-commit": "^1.0.0",
    "postcss": "^8.4.47",
    "postcss-cli": "^8.3.1",
    "postcss-css-variables": "^0.18.0",
    "postcss-custom-media": "^8.0.0",
    "postcss-custom-properties": "^14.0.1",
    "postcss-font-magician": "^4.0.0",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^10.0.5"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "suitcss-base": "^5.0.0"
  }
}
